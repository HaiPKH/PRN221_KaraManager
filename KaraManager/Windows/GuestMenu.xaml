<Window x:Class="KaraManager.GuestMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KaraManager"
        mc:Ignorable="d"
        Title="GuestMenu" Height="450" Width="800"
        ResizeMode="NoResize">
    <Grid>

        <Label Content="KaraManager(Guest)" HorizontalAlignment="Center" Margin="0,39,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.512,0.533"/>
        <Button x:Name="btnGuestLogout" Content="Logout" HorizontalAlignment="Center" Margin="0,338,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.5,0.5" Click="btnGuestLogout_Click"/>
        <Button x:Name="btnMessage" Content="Messages" HorizontalAlignment="Center" Margin="0,125,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.52,0.563" Height="35" Width="104" Click="btnMessage_Click"/>
    </Grid>
</Window>
